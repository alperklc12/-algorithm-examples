def getNthFib(n):
    x = 0
    y = 1
    fib_num = 0
    if n == 2:
        return 1
    elif n == 1:
        return 0
    else:
        for i in range(2, n):
            fib_num = x + y
            x, y = y, fib_num
    return fib_num


